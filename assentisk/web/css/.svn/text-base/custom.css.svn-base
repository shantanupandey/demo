    -moz-border-radius: 0;
    -webkit-border-radius: 0;
    border-radius: 0;
    height: 50px;
    font-size: 14px;
	padding-left: 40px;
	border-left: 1px solid #eee;
	margin-left: 20px;
}

.searchform input:focus {
